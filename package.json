{
  "name": "poz",
  "version": "0.0.4",
  "description": "Programmable scaffolding generator",
  "repository": {
    "url": "ULIVZ/poz",
    "type": "poz"
  },
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test:cov": "BABEL_ENV=test jest src/** --coverage",
    "test": "BABEL_ENV=test jest src/**",
    "lint": "xo",
    "prepublish": "npm run build",
    "build": "bili src/{index,cli}.js --filename [name].js --banner --plugin json --exports named --js babel --no-babel.babelrc",
    "example": "poi --config build/poi.config.js",
    "build:example": "poi build --config build/poi.config.js",
    "gh": "gh-pages -d example/dist",
    "deploy": "npm run build:example && npm run gh",
    "website": "w7 ./docs -e docs/index.html"
  },
  "author": "ULIVZ <chl814@foxmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "packages"
    ]
  },
  "dependencies": {
    "alphax": "1.0.3",
    "archy": "^1.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "bili": "^3.0.15",
    "cac": "^4.2.4",
    "chalk": "^2.3.0",
    "download-git-repo": "^1.0.1",
    "download-npm-package": "^3.1.11",
    "fs-extra": "^4.0.2",
    "git-config-path": "^1.0.1",
    "inquirer": "^3.3.0",
    "jstransformer": "^1.0.0",
    "jstransformer-handlebars": "^1.1.0",
    "jstransformer-swig": "^1.0.0",
    "minimatch": "^3.0.4",
    "ora": "^1.3.0",
    "parse-git-config": "^1.1.1",
    "shelljs": "^0.8.1",
    "text-table": "^0.2.0",
    "user-home": "^2.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^4.0.5",
    "@types/inquirer": "0.0.35",
    "@types/node": "^8.0.47",
    "@types/vinyl": "^2.0.1",
    "babel-preset-env": "^1.6.1",
    "eslint-config-ulivz": "^1.0.0",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "jest-cli": "^21.2.1",
    "rollup-plugin-json": "^2.3.0",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "ulivz",
    "envs": [
      "jest"
    ]
  }
}
